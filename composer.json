{
  "name": "lag/adminbundle",
  "type": "symfony-bundle",
  "description": "Generic admin bundle for Symfony2 from L'arri√®re-guichet",
  "keywords": [
    "Admin",
    "Backoffice",
    "Symfony2",
    "Generic"
  ],
  "homepage": "https://github.com/larriereguichet/AdminBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "JohnKrovitch",
      "email": "arnaudfrezet@gmail.com"
    },
    {
      "name": "JohanDufour",
      "email": "johan.dufour@gmail.com"
    },
    {
      "name": "VincentChalnot",
      "email": "vincent.chalnot@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "johnkrovitch/configuration": "^1.3",
    "johnkrovitch/orm-pack": "^0.2",
    "lag/string-utils": "^1.2",
    "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
    "symfony/dependency-injection": "^3.0|^4.0",
    "symfony/form": "^3.0|^4.0",
    "symfony/framework-bundle": "^3.0|^4.0",
    "symfony/security-bundle": "^3.0|^4.0",
    "symfony/translation": "^3.0|^4.0",
    "twig/extensions": "^1.2",
    "twig/twig": "^2.0",
    "white-october/pagerfanta-bundle": "^1.0"
  },
  "require-dev": {
    "nyholm/symfony-bundle-test": "^1.6",
    "phpstan/phpstan": "^0.12.4",
    "phpunit/phpunit": "^6.0",
    "roave/backward-compatibility-check": "^3.0",
    "sensiolabs/security-checker": "^3.0|^4.0|^5.0|^6.0",
    "symfony/monolog-bundle": "^3.0|^4.0",
    "symfony/serializer": "^3.0|^4.0",
    "symfony/var-dumper": "^3.0|^4.0",
    "symfony/webpack-encore-bundle": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "LAG\\AdminBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LAG\\AdminBundle\\Tests\\": "tests/AdminBundle/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
      "security-checker security:check": "script"
    }
  }
}
