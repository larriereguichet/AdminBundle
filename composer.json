{
    "name": "lag/adminbundle",
    "type": "symfony-bundle",
    "description": "Generic admin bundle for Symfony2 from L'arri√®re-guichet",
    "keywords": [
        "Admin",
        "Backoffice",
        "Symfony2",
        "Generic"
    ],
    "homepage": "https://github.com/larriereguichet/AdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "JohnKrovitch",
            "email": "arnaudfrezet@gmail.com"
        },
        {
            "name": "JohanDufour",
            "email": "johan.dufour@gmail.com"
        },
        {
            "name": "VincentChalnot",
            "email": "vincent.chalnot@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "johnkrovitch/configuration": "^2.2",
        "johnkrovitch/orm-pack": "^3.1",
        "knplabs/knp-menu-bundle": "^3.2",
        "symfony/dependency-injection": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.1",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nyholm/symfony-bundle-test": "^1.6",
        "php-parallel-lint/php-var-dump-check": "^0.5",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "sensiolabs/security-checker": "^4.0|^5.0|^6.0",
        "symfony/browser-kit": "^4.4|^5.2",
        "symfony/console": "^4.4|^5.2",
        "symfony/css-selector": "^4.4|^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/var-dumper": "^4.4|^5.2",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "LAG\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LAG\\AdminBundle\\Tests\\": "tests/phpunit/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },

    "scripts": {
        "auto-scripts": {
            "security-checker security:check": "script",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
