{% extends 'knp_menu.html.twig' %}

{% block label %}
    {% if item.extras.icon|default(false) %}
        <i class="bi bi-{{ item.extras.icon }} me-2"></i>
    {% endif %}

    {% if options.allow_safe_labels and item.getExtra('safe_label', false) %}
        {{ item.label|raw }}
    {% else %}
        {{ item.label|admin_trans }}
    {% endif %}
{% endblock %}
