<a href="{{ lag_admin_resource_url(options, data) }}" {{ attributes }}>
    {% if options.icon %}<i class="bi bi-{{ options.icon }}"></i>{% endif %}

    {% set text = data.(options.textPath)|default(options.text)|default(data) %}

    {% if options.translatable %}
        {{ text|trans({}, options.translationDomain) }}
    {% else %}
        {{ text }}
    {% endif %}
</a>
