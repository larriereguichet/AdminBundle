<div id="tab-application"
     class="tab">
    <h1>Application</h1>

    <table>
        <thead>
        <tr>
            <th class="key">Key</th>
            <th class="value">Value</th>
        </tr>
        </thead>
        <tbody>
        {% for key, value in collector.data.application %}
            <tr>
                <td>{{ key }}</td>
                <td>{{ value }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
